@{
    Layout = "_Layout";
}
<title>About us - Outsourcy</title>
<!-- Banner Section-->
<section class="banner-section-four -type-16"
         style="background-image: url(/images/background/about-us-banner.jpg);">
    <div class="auto-container">
        <div class="row">
            <div class="content-box">
                <div class="content-column col-12">
                    <div class="inner-column">
                        <div class="title-box text-start wow fadeInUp" data-wow-delay="300ms">
                            <h3 class="text-primary">About Us</h3>
                            <div class="text fs-5">Know Our Story And How We Work.</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</section>
<!-- End Banner Section-->
<!-- Features Section-->
<section class="features__section">
    <div class="auto-container">
        <div class="row features__grid justify-content-between">

            <div class="col-lg-3 col-md-6">
                <div class="features -type-1">
                    <div class="icon-wrap">
                        <div class="icon icon-case"></div>
                    </div>
                    <div class="title">Register an account to start</div>
                </div>
            </div>


            <div class="col-lg-auto features-line-col">
                <div class="features-line"></div>
            </div>


            <div class="col-lg-3 col-md-6">
                <div class="features -type-1">
                    <div class="icon-wrap">
                        <div class="icon icon-contact"></div>
                    </div>
                    <div class="title">Explore over thousands of resumes</div>
                </div>
            </div>


            <div class="col-lg-auto features-line-col">
                <div class="features-line"></div>
            </div>


            <div class="col-lg-3 col-md-6">
                <div class="features -type-1">
                    <div class="icon-wrap">
                        <div class="icon icon-doc"></div>
                    </div>
                    <div class="title">Find the most suitable candidate</div>
                </div>
            </div>


        </div>
    </div>
</section>
<!-- End Features Section-->
<!-- About Section -->
<section id="section-2" class="about-section-two style-two layout-pt-60 layout-pb-60 js-tab-menu-content">
    <div class="auto-container">
        <div class="row grid-base align-items-center">
            <!-- Content Column -->
            <div class="content-column col-xl-7 col-lg-7 col-md-12 col-sm-12">
                <div class="wow fadeInLeft">
                    <div class="sec-title">
                        <h2 class="fw-700">OUTSOURCY</h2>
                        <div class="text mt-30  fs-6">
                            OUTSOURCY is one of the leading Integrated Human Resources Outsourcing Solutions
                            (HRO) / Business Process Outsourcing
                            (BPO) / Adminstartion Process Outsourcing (APO) Recruitment, Staffing, Hiring
                            and HR Consultancy service providers based
                            in Cairo, Egypt. Founded in 2016.
                        </div>
                        <p class="text mt-2 fw-700">
                            We have supported a wide range of international & local companies from various
                            kinds of industries to:
                        </p>
                        <ul class="list-style-one mt-24 mb-24">
                            <li>Increase profitability.</li>
                            <li>Maximize employee productivity.</li>
                            <li>Reduce time spent on HR activities.</li>
                            <li>Lower employment-related liability.</li>
                            <li>Minimize labor costs.</li>
                            <li>Grow their busines.</li>
                        </ul>
                    </div>
                    <div class="d-flex w-75 align-items-center justify-content-between">
                        <a href="#" class="theme-btn btn-style-one">Collaboration</a>
                        <a href="#" class="theme-btn btn-style-one">Our Services</a>
                    </div>
                </div>
            </div>
            <!-- Image Column -->
            <div class="image-column  col-xl-5 col-lg-5 col-md-12 col-sm-12 wow fadeInRight">
                <figure class="image-box"><img src="~/images/section/about-us-section.jpg" alt=""></figure>
            </div>
        </div>
    </div>
</section>
<!-- End About Section -->
<!-- Call To Action Two -->
<section class="call-to-action-two" style="background-image: url(/images/background/1.jpg);">
    <div class="auto-container wow fadeInUp">
        <div class="sec-title light text-center">
            <h2>Join us & Hire Talent</h2>
            <div class="text">Your Dream Jobs Are Waiting</div>
        </div>

        <div class="btn-box">
            <a href="#" class="theme-btn btn-style-three">Search Job</a>
            <a href="#" class="theme-btn btn-style-two">Get a Free Trail</a>
        </div>
    </div>
</section>
<!-- End Call To Action -->
<!-- About Section -->
<section id="section-2" class="about-section-two style-two js-tab-menu-content">
    <div class="auto-container">
        <div class="row grid-base align-items-center">
            <!-- Image Column -->
            <div class="image-column -no-margin col-xl-6 col-lg-6 col-md-12 col-sm-12 wow fadeInRight">
                <figure class="image-box"><img src="~/images/section/candiate-01.jpg" alt=""></figure>
            </div>

            <!-- Content Column -->
            <div class="content-column col-xl-6  col-lg-6  col-md-12 col-sm-12">
                <div class="wow fadeInLeft">
                    <div class="sec-title">
                        <h3 class=" fw-700 text-primary">HOW WE HELP YOUR BUSINESS?</h3>
                        <div class="text mt-30 mw-100">
                            We know that every business is unique and has different business objectives based on operational differences.
                            Outsourcing is the key to establishing your operations compliantly and quickly. OUTSOURCY can customize human resource
                            solutions based on the exact needs of your company. Every employee at OUTSOURCY is empowered to ensure the success of
                            our clients – we strive for excellence in all tasks.
                        </div>
                        <h3 class="fw-700 text-primary mt-30">OUR APPROACH</h3>
                        <div class="text mt-30 mw-100">
                            We know that every business is unique and has different business objectives based on operational differences.
                            Outsourcing is the key to establishing your operations compliantly and quickly. OUTSOURCY can customize human
                            resource
                            solutions based on the exact needs of your company. Every employee at OUTSOURCY is empowered to ensure the success
                            of
                            our clients – we strive for excellence in all tasks.
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
<!-- End About Section -->
<!-- Subscribe Section -->
<section class="subscribe-section -type-2">
    <div class="auto-container">
        <div class="outer-box wow fadeInUp">
            <div class="sec-title mb-0">
                <h2>Subscribe Our Newsletter</h2>
                <div class="text">
                    Advertise your jobs to thousands of monthly users and search
                </div>
            </div>

            <div class="form-column md:mt-20">
                <div class="subscribe-form">
                    <form method="post" action="#">
                        <div class="form-group">
                            <div class="response"></div>
                        </div>
                        <div class="form-group">
                            <input type="email" name="email" class="email" value="" placeholder="Your e-mail" required>
                            <button type="button" id="subscribe-newslatters"
                                    class="theme-btn btn-style-seven">
                                Subscribe
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Subscribe Section -->
<!-- Testimonial Section -->
<section class="layout-pt-120 layout-pb-60 testimonial-section style-two">
    <div class="auto-container">
        <!-- Sec Title -->
        <div class="sec-title text-center">
            <h2 class="fw-700">WHAT WE STAND FOR</h2>

        </div>

        <div class="row grid-base">

            <div class="col-xl-4 col-lg-4 col-md-6 d-flex align-items-stretch">
                <div class="step-item text-center">
                    <div class="image">
                        <img src="~/images/section/our-mission.jpg" alt="image">
                    </div>
                    <h3 class="title">OUR VISION</h3>
                    <p class="text">
                        To become the leading outsourcing agency in the nearest future and export professional integrated HR solutions and be
                        the most efficient bridge between the Human Resource and our Partners.
                    </p>
                </div>
            </div>

            <div class="col-xl-4 col-lg-4 col-md-6 d-flex align-items-stretch">
                <div class="step-item text-center">
                    <div class="image">
                        <img src="~/images/section/our-vision.jpg" alt="image">
                    </div>
                    <h3 class="title">OUR MISSION</h3>
                    <p class="text">
                        We are a professional, enthusiastic and innovative team, dedicated to providing professional HR solutions that help our
                        partners become more productive and profitable
                    </p>
                </div>
            </div>

            <div class="col-xl-4 col-lg-4 col-md-6 d-flex align-items-stretch">
                <div class="step-item text-center">
                    <div class="image">
                        <img src="~/images/section/our-values.jpg" alt="image">
                    </div>
                    <h3 class="title">
                        OUR VALUES
                    </h3>
                    <p class="text">
                        INTEGRITY & LOYALTY _ QUALITY-ORIENTATED
                        ACCOUNTABILITY _ EXCELLENCE _ TOGETHER
                    </p>
                </div>
            </div>

        </div>
    </div>
</section>
<!-- End Testimonial Section -->
